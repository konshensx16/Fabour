{
	"name": 	"Filezilla Launcher extension",
	"version": 	"1.0",
	"description":	"ERP is shit and this extension might help make it less shitier",
	"content_scripts": [
		{
			"matches": ["http://erp.wewebcom.fr/*", "https://erp.wewebcom.fr/*"],
			"run_at": "document_idle",
			"js": 	   ["contentScript.js"]
		}
	],
	"background": {
		"scripts": ["background.js"],
		"persitent": false
	},
	"icons": {
	  	"128": "icon-128.png"
	},
	"permissions": [
		"nativeMessaging"
	],
	"browser_action": {},
	"manifest_version": 2
}